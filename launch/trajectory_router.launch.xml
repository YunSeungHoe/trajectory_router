<launch>
  <arg name="table_name" default="240720_kiapi_trajectory_table.yaml" />
  <arg name="trajectory_table_param_path" default="$(find-pkg-share trajectory_router)/config/$(var table_name)"/>
  <node pkg="trajectory_router" exec="trajectory_router" name="trajectory_router_node" output="screen">
    <param from="$(var trajectory_table_param_path)"/>
  </node>
</launch>
